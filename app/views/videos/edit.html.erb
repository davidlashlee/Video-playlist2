<p> this is the edit page </p>

<%= form_for :video, url: video_path(@video), method: :patch do |f| %>

	<p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :link %><br>
    <%= f.text_area :link %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
 
<%= link_to 'Back', video_path %>